void setup() {
  Serial.begin(9600);

  // SSD1306_SWITCHCAPVCC = generate display voltage from 3.3V internally
  if(!display.begin(SSD1306_SWITCHCAPVCC, 0x3D)) { // Address 0x3D for 128x64
    Serial.println(F("SSD1306 allocation failed"));
    for(;;); // Don't proceed, loop forever
  }

  // Clear the buffer
  display.clearDisplay();
}

void loop() {
  display.clearDisplay();
  display.drawBitmap(0, 0,  MakeabilityLabLogoWhiteBackground, 128, 64, WHITE);
  display.display();
  delay(1000); 

  display.clearDisplay();
  display.drawBitmap(0, 0,  MakeabilityLabLogoBlackBackground, 128, 64, WHITE);
  display.display();
  delay(1000); 
}
